# Store final cell length for strain calculations
variable tmp equal "lz"
variable L0 equal ${tmp}
variable L0 equal 67.6158468618598
print "Initial Length, L0: ${L0}"
Initial Length, L0: 67.6158468618598

######################################
# DEFORMATION
reset_timestep	0

fix		1 all npt temp 300 300 1 x 0 0 1 y 0 0 1 drag 1
variable srate equal 1.0e10
variable srate1 equal "v_srate / 1.0e12"
fix		2 all deform 1 z erate ${srate1} remap x units box
fix		2 all deform 1 z erate 0.01 remap x units box

# Output strain and stress info to file
# for units metal, pressure is in [bars] = 100 [kPa] = 1/10000 [GPa]
# p2, p3, p4 are in GPa
variable strain equal "(lz - v_L0)/v_L0"
variable p1 equal "v_strain"
variable p2 equal "-pxx/10000"
variable p3 equal "-pyy/10000"
variable p4 equal "-pzz/10000"
fix def1 all print 100 "${p1} ${p2} ${p3} ${p4}" file SiNT_100.def1.txt screen no

# Use cfg for AtomEye
dump 		1 all custom 100 dump.tensile id type x y z c_csym c_peratom fx fy fz


# Display thermo
thermo 	200
thermo_style	custom step v_strain temp v_p2 v_p3 v_p4 ke pe press

run		10000
